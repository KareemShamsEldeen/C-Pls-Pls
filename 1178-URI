#include <iostream>
#include <iomanip>
 
using namespace std;
 
int main() {
 
    
   int size=100;
   double N[size];
   double x;


   cin>>x;

    for(int i=0;i<size;i++)
   {
       cout<<"N["<<i<<"]"<<" = "<<fixed<<setprecision(4)<<x<<endl;
       x=x/2;

   }
    return 0;
}
