# C-
my solved C++ problems
#include <iostream>
 
using namespace std;
 
void serch(long long a[],int size, int x)
{
 
bool ser=false;
int counter;
 for(int i=0;i<size;i++)
  {
      if(a[i]==x)
      {
          ser=true;
          counter=i;
          break;
 
      }
  }
 
  if(ser)
  {
      cout<<counter;
  }
  else
  {
      cout<<"-1";
  }
 
}
 
 
 
int main()
{
  int size,x;
  cin>>size;
 
  long long a[size];
  for(int i=0;i<size;i++)
  {
      cin>>a[i];
  }
 
  cin>>x;
 
  serch(a,size,x);
 
    return 0;
}
