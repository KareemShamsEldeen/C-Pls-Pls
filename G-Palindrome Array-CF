# C-
my solved C++ problems
#include <iostream>
 
using namespace std;
 
void pal(long long a[],int size)
{
 
 
bool pli=true;
 for(int i=0,j=size-1;i<size;i++,j--)
  {
      if(a[i]!=a[j])
        pli=false;
  }
if(pli)
        cout<<"YES";
      else
      {
          cout<<"NO";
      }
}
 
 
 
int main()
{
  int size;
  cin>>size;
 
  long long a[size];
  for(int i=0;i<size;i++)
  {
      cin>>a[i];
  }
 
 
 
  pal(a,size);
 
    return 0;
}
