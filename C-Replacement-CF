# C-
my solved C++ problems
#include <iostream>
 
using namespace std;
 
void rep(long long a[],int size)
{
 
 
 
 for(int i=0;i<size;i++)
  {
      if(a[i]>0)
      {
          a[i]=1;
      }
      else if(a[i]==0)
      {
          a[i]=0;
      }
      else
      {
          a[i]=2;
      }
  }
 
   for(int i=0;i<size;i++)
  {
      cout<<a[i]<<" ";
  }
}
 
 
 
int main()
{
  int size;
  cin>>size;
 
  long long a[size];
  for(int i=0;i<size;i++)
  {
      cin>>a[i];
  }
 
 
 
  rep(a,size);
 
    return 0;
}
